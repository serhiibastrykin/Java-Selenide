<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UI Tests" parallel="methods" thread-count="2">
    <test name="selenide-tests">
        <packages>
