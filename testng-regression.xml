            </class>
            <class name="com.seleniumeasy.tests.alertsandmodals.WindowPopupModalTest">
                <methods>
                    <exclude name=".*"/>
                </methods>
            </class>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->